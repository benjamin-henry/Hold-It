{
  "expo": {
    "name": "Hold-It !",
    "slug": "hold-it",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brain_icon.png",
    "splash": {
      "image": "./assets/smart_baboon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "web": {
      "favicon": "./assets/brain_icon.png"
    },
    "androidNavigationBar": {
      "visible":true,
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    }
  }
}
